#include<iostream>
using namespace std;
class Volume			
{
private:
	int L;
	int W;
	int H;
public:
	void InputData(); 
	void VolumeCalculation();
};



void Volume::InputData()
{
	cout << "长：";
	cin >> L;
	cout << "宽：";
	cin >> W;
	cout << "高：";
	cin >> H;
}
void Volume::VolumeCalculation()
{
	cout << "体积:" << (L * W * H) << endl;
}


int main()
{
	Volume v;
	int n=0,i;
	cout << "请输入要求长方体的个数" << endl;
	cin>>n;
	for (int i = 0; i < n; i++) 
	{
		v.InputData();
		v.VolumeCalculation();
	}
	return 0;
	}
